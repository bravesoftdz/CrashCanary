<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fairy.easy.crashcanary">

    <application>
        <activity
            android:name=".CrashInfoActivity"
            android:enabled="false"
            android:icon="@drawable/crash_canary_icon"
            android:label="@string/crash_canary_display_activity_label"
            android:taskAffinity="fairy.easy.crashcanary"
            android:theme="@style/crash_canary_LogCanary.Base">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>